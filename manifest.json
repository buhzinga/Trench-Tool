{
  "manifest_version": 3,
  "name": "Trench Tool",
  "version": "1.0",
  "description": "Photon Memescope Ticker/Name Copy",
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.tinyastro.io/*",
        "*://photon-sol.tinyastro.io/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}